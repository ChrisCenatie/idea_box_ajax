<div class="row">
  <div class="col-sm-4">
  </div>
  <div class="col-sm-8">
    <h1>Idea Box</h1>
  </div>
</div>
<div class="container-fluid">
  <div class="row-center">
    <div class="col-sm-8">
      <form class="form-horizontal">
        <div class="form-group">
          <label for="inputTitle" class="col-sm-2 control-label">Title</label>
          <div class="col-sm-10">
            <input type="title" class="form-control" id="title" placeholder="Title">
          </div>
        </div>
        <div class="form-group">
          <label for="inputBody" class="col-sm-2 control-label">Body</label>
          <div class="col-sm-10">
            <input type="body" class="form-control" id="body" placeholder="Body">
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-default" id="create-idea">Save</button>
          </div>
        </div>
      </form>
    </div>
    <div class="col-sm-4">
    </div>
  </div>
</div>
<div class="container-fluid">
  <h2>Current Ideas</h2>
  <hr size="30">
  <div class="row-center">
    <div class="col-sm-2">
    </div>
    <div class="col-sm-10" id="latest-ideas">
    </div>


    <div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
      <div class="modal-dialog modal-lg">
        <div class="modal-content">
        </div>
      </div>
    </div>



  </div>
</div>
